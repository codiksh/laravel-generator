{
    "name": "codiksh/laravel-generator",
    "type": "library",
    "description": "Codiksh Laravel Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Nikhil Bhatia",
        "email": "hello@nikhilbhatia.in"
    }],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "laracasts/flash": "^3.2",
        "spatie/laravel-html": "^3.2",
        "symfony/var-exporter": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.22|^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^1.2|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Codiksh\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Codiksh\\Generator\\CodikshGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
